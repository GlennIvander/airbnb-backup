<div class="flex flex-col w-full">
  <%= link_to "< Back", @listing, class:"bg-gray-100 rounded-lg border-1 border-gray-900 mr-auto p-2 hover:bg-gray-300" %>
  <br/>
  <div class="grid md:grid-cols-4 gap-4">
    <% @listing.images.each do |image| %>
      <%= image_tag image, class: "w-full md:w-96 h-64 object-cover" %>
    <% end %>
  </div>
</div>